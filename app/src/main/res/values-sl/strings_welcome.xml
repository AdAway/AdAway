    <string name="welcome_sync_header">Sinhronizacija</string>
    <string name="welcome_synced_header">Sinhronizirano!</string>
    <string name="welcome_sync_summary">AdAway iz spletnih virov prenese oglasna omrežja, ki jih blokira. Kasneje jih lahko prilagodite v nastavitvah.</string>
